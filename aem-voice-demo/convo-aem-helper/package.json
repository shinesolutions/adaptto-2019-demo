  },
  "dependencies": {
    "lodash": "^4.7.11"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
