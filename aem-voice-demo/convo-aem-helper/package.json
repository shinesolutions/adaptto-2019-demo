{
  "name": "convo-aem-helper",
  "version": "0.0.1-pre.4",
  "description": "Helper node.js module for Convo AEM",
  "keywords": [
    "convo",
    "aem"
  ],
  "main": "./lib/index",
  "scripts": {
    "test": "node_modules/.bin/mocha --timeout 5000 test/*/*.js",
